[Unit]
Description=Create ssh and http tunnel to the server
After=network-online.target network-wait-online.target multi-user.target
Wants=network-online.target network-wait-online.target

[Install]
WantedBy=ssh_tunnel.target http_tunnel.target

[Service]
ExecStart=/bin/bash /home/pi/car_audio/raspbian/init.sh
Type=simple
User=pi
WorkingDirectory=/home/pi
Restart=on-failure
RestartSec=5

